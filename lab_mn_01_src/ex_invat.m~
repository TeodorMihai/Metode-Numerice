v = [ 1 2 3 4 5];
s = 0;
for x = v 
	s = s + x;
endfor
disp("Media este");
disp(s / length(v));

g = 2;

while g < 1000
	disp(g)
	g = g + 1;
	g = g * 2;
endwhile
